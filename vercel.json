{
  "framework": {
    "version": 2,
    "build": {
      "command": "npm run build",
      "output": "client/build"
    }
  },
  "rewrites": [
    {
      "source": "/server/(.*)",
      "destination": "/server/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/build/$1"
    }
  ]
}
